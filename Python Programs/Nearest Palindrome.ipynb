    "import sys\n",
    "def nearest_palindrome(num):\n",
    "    for i in range(num+1,sys.maxsize):\n",
    "        if str(i)==str(i)[::-1]:\n",
    "            return i  \n",
    "n=int(input())\n",
    "print(nearest_palindrome(n))\n"
   ]
